<div class="login-wrapper" id="login-content">
    <div class="login-content">
        <a href="#" class="close">x</a>
        <h3>Login</h3>
		<p style="text-align: center;"id="response"></p>
        <form method="post" action="/user/login" id="form-login-2">

			<div class="row">
                <label for="email">
                  Email:
					<input type="email" class="form-control" id="email" name="email" placeholder="Nhập email..."  aria-describedby="emailHelp"  >
				</label>
				
            </div>
            <div class="row">
            	<label for="password">
                    Password:
                    <input type="password" class="form-control" id="password" name="password" placeholder="Nhập mật khẩu..." required pattern="\S+.*" >
                </label>
            </div>
            <div class="row">
            	<div class="remember">
